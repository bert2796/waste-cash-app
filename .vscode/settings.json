{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.insertSpaces": false,
  "files.insertFinalNewline": true,
  "files.eol": "\n",
  "files.watcherExclude": {
    "coverage": true,
    "node_modules": true,
    "**/node_modules": true
  },
  "cSpell.words": [
    "nestjs",
    "fastify"
  ]
}
